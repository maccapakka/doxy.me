    "@manypkg/cli": "^0.21.4",
    "husky": "^9.1.7",
    "knip": "^5.71.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "sort-package-json": "^3.5.0",
    "turbo": "^2.6.3",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
